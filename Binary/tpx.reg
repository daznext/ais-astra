Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\.tpx]
@="TPXfile"
"Content Type"="text/plain"

[HKEY_CLASSES_ROOT\TPXfile\Shell\Open\command]
@="Z:\\home\\wineuser\\ais\\AKUZ.UserArm.exe tpx \"%1\"" 

[HKEY_CLASSES_ROOT\TPXfile\Shell\Print\command]
@="Z:\\home\\wineuser\\ais\\AKUZ.UserArm.exe tpx /p \"%1\"" 

[HKEY_CLASSES_ROOT\TPXfile\Shell\PrintTo\command]
@="Z:\\home\\wineuser\\ais\\AKUZ.UserArm.exe tpx /p \"%1\" \"%2\"" 


